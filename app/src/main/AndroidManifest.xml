<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dressme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.dressMe">
        <activity
            android:name="Activities.AddDress"
            android:exported="false" />
        <activity
            android:name="Activities.SupplierRegister"
            android:exported="false" />
        <activity
            android:name="Activities.SupplierDresses"
            android:exported="false" />
        <activity
            android:name="Activities.MainSupplier"
            android:exported="false" />
        <activity
            android:name="Activities.ClientRegister"
            android:exported="false" />
        <activity
            android:name="Activities.loginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>